{
  // Language specific
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "code-runner.executorMap": {
    "FortranFreeForm": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "ahk": "autohotkey",
    "applescript": "osascript",
    "autoit": "autoit3",
    "bat": "cmd /c",
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "clojure": "lein exec",
    "coffeescript": "coffee",
    "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "crystal": "crystal",
    "csharp": "scriptcs",
    "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
    "dart": "dart",
    "fortran": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran-modern": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran_fixed-form": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fsharp": "fsi",
    "go": "go run",
    "groovy": "groovy",
    "haskell": "runhaskell",
    "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
    "java": "cd $dir && javac $fileName && java $fileName",
    "javascript": "node",
    "julia": "julia",
    "kit": "kitc --run",
    "less": "cd $dir && lessc $fileName $fileNameWithoutExt.css",
    "lisp": "sbcl --script",
    "lua": "lua",
    "nim": "nim compile --verbosity:0 --hints:off --run",
    "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "ocaml": "ocaml",
    "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
    "perl": "perl",
    "perl6": "perl6",
    "php": "php",
    "pml": "spin",
    "powershell": "powershell -ExecutionPolicy ByPass -File",
    "python": "py -u",
    "r": "Rscript",
    "racket": "racket",
    "ruby": "ruby",
    "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
    "sass": "sass --style expanded",
    "scala": "scala",
    "scheme": "csi -script",
    "scss": "scss --style expanded",
    "shellscript": "bash",
    "swift": "swift",
    "typescript": "ts-node",
    "v": "v run",
    "vbscript": "cscript //Nologo"
  },
  "code-runner.executorMapByFileExtension": {
    ".ahk": "autohotkey",
    ".au3": "autoit3",
    ".cr": "crystal",
    ".csproj": "dotnet run --project",
    ".cu": "cd $dir && nvcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    ".d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
    ".dart": "dart",
    ".exs": "elixir",
    ".fsproj": "dotnet run --project",
    ".hs": "runhaskell",
    ".hx": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
    ".jl": "julia",
    ".kit": "kitc --run",
    ".kt": "cd $dir && kotlinc $fileName -include-runtime -d $fileNameWithoutExt.jar && java -jar $fileNameWithoutExt.jar",
    ".kts": "kotlinc -script",
    ".lisp": "sbcl --script",
    ".ml": "ocaml",
    ".nim": "nim compile --verbosity:0 --hints:off --run",
    ".pas": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
    ".pml": "spin",
    ".pp": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
    ".rkt": "racket",
    ".sass": "sass --style expanded",
    ".scala": "scala",
    ".scm": "csi -script",
    ".v": "v run",
    ".vb": "cd $dir && vbc /nologo $fileName && $dir$fileNameWithoutExt",
    ".vbs": "cscript //Nologo",
    ".vsh": "v run"
  },
  "code-runner.ignoreSelection": true,
  "code-runner.runInTerminal": true,
  "code-runner.saveFileBeforeRun": true,

  // Editor
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.accessibilitySupport": "off",
  "editor.autoClosingDelete": "always",
  "editor.bracketPairColorization.enabled": false,
  "editor.cursorSurroundingLines": 10,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.foldingImportsByDefault": true,
  "editor.fontFamily": "RobotoMono Nerd Font",
  "editor.fontSize": 16,
  "editor.formatOnPaste": true,
  "editor.guides.bracketPairs": "active",
  "editor.hover.delay": 500,
  "editor.inlineSuggest.enabled": true,
  "editor.lineNumbers": "relative",
  "editor.minimap.maxColumn": 80,
  "editor.minimap.showSlider": "mouseover",
  "editor.minimap.size": "fill",
  "editor.renderWhitespace": "all",
  "editor.rulers": [
    {
      "color": "#f18ab585",
      "column": 80
    }
  ],
  "editor.semanticHighlighting.enabled": true,
  "editor.semanticTokenColorCustomizations": {
    "enabled": true,
    "rules": {
      "comment": "#d5d363",
      "function": "#9ecba3",
      "method": "#9ecba3",
      "macro": "#9ecba3",
      "decorator": "#9ecba3",
      "builtinAttribute": "#9ecba3",
      "class": "#c6d0f5",
      "struct": "#c6d0f5",
      "enum": "#c6d0f5",
      "enumMember": "#c6d0f5",
      "interface": "#c6d0f5",
      "derive": "#c6d0f5",
      "variable.constant": "#cd87ed",
      "type": "#cd87ed",
      "parameter": "#e2c5d2",
      "namespace": "#e2c5d2",
      "variable.defaultLibrary": "#db6698",
      "keyword": "#db6698"
    }
  },
  "editor.stickyScroll.enabled": true,
  "editor.suggest.preview": true,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.tokenColorCustomizations": {
    "comments": "#d5d363",
    "functions": "#9ecba3",
    "keywords": "#db6698",
    "numbers": "#cd87ed",
    "strings": "#ce9178",
    "textMateRules": [
      {
        "name": "Class",
        "scope": ["support.class.component"],
        "settings": {
          "foreground": "#c6d0f5"
        }
      },
      {
        "name": "Keyword",
        "scope": ["entity.name.function"],
        "settings": {
          "foreground": "#db6698"
        }
      },
      {
        "name": "Tags",
        "scope": ["keyword.other", "entity.name.tag"],
        "settings": {
          "foreground": "#e2c5d2"
        }
      },
      {
        "name": "Attributes, Constant",
        "scope": [
          "entity.other.attribute-name",
          "constant.language.json.comments",
          "constant.language.boolean",
          "types"
        ],
        "settings": {
          "foreground": "#cd87ed"
        }
      },
      {
        "name": "Operator, Syntax",
        "scope": [
          "keyword.operator",
          "punctuation.section.embedded",
          "punctuation.definition",
          "punctuation.definition.template-expression",
          "punctuation",
          "meta.brace"
        ],
        "settings": {
          "foreground": "#9e9e9e"
        }
      },
      {
        "name": "String",
        "scope": ["string.quoted.double.json.comments"],
        "settings": {
          "foreground": "#ce9178"
        }
      }
    ],
    "types": "#e2c5d2",
    "variables": "#e2c5d2"
  },
  "editor.wordWrap": "wordWrapColumn",

  // File
  "emmet.excludeLanguages": ["markdown", "php"],
  "files.associations": {
    "*.embeddedhtml": "html",
    "*.rmd": "markdown",
    "*.sql": "sql"
  },
  "files.autoSave": "onFocusChange",
  "files.eol": "\n",
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.git": false,
    "**/.project": true,
    "**/.settings": true
  },

  // Git
  "git.allowForcePush": true,
  "git.autofetch": true,
  "git.openRepositoryInParentFolders": "always",
  "git.statusLimit": 0,
  "gitlens.hovers.avatars": false,
  "gitlens.hovers.detailsMarkdownFormat": "${author} ${ago}${' via 'pullRequest} &nbsp; _(${date})_ \n\n${message}${\n\n---\n\nfootnotes}\n\n${commands}",
  "gitlens.statusBar.tooltipFormat": "${author} ${ago}${' via 'pullRequest} &nbsp; _(${date})_ \n\n${message}${\n\n---\n\nfootnotes}\n\n${commands}",
  "gitlens.views.contributors.avatars": false,
  "gitlens.views.contributors.files.layout": "tree",
  "gitlens.views.contributors.showAllBranches": true,

  // Other
  "glassit.alpha": 235,
  "glassit.step": 20,
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "liveServer.settings.useLocalIp": true,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "notebook.lineNumbers": "on",
  "path-intellisense.showHiddenFiles": true,
  "php.suggest.basic": false,
  "php.validate.enable": false,
  "prettier.endOfLine": "auto",
  "problems.showCurrentInStatus": true,
  "redhat.telemetry.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "telemetry.feedback.enabled": false,
  "telemetry.telemetryLevel": "off",

  // Terminal
  "terminal.external.windowsExec": "Terminal.app",
  "terminal.integrated.allowChords": false,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.profiles.windows": {
    "Window PowerShell": {
      "args": [],
      "path": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
    }
  },
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.shellIntegration.enabled": true,
  "terminal.integrated.tabs.enabled": false,

  // Vim
  "vim.enableNeovim": false,
  "vim.foldfix": true,
  "vim.handleKeys": {
    "<C-c>": false
  },
  "vim.insertModeKeyBindings": [
    {
      "after": ["<Esc>"],
      "before": ["j", "j"]
    },
    {
      "after": ["<Esc>"],
      "before": ["j", "k"]
    },
    {
      "after": ["\"", "+", "y"],
      "before": ["<C-c>"]
    }
  ],
  "vim.leader": " ",
  "vim.mouseSelectionGoesIntoVisualMode": true,
  // "vim.neovimConfigPath": "D:\\config\\nvim\\init.lua",
  "vim.neovimUseConfigFile": false,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.view.explorer"]
    },
    {
      "after": ["g", "j"],
      "before": ["j"]
    },
    {
      "after": ["g", "k"],
      "before": ["k"]
    },
    {
      "before": ["<C-j>"],
      "commands": ["workbench.action.terminal.focus"]
    },
    {
      "before": ["g", "i"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["g", "r"],
      "commands": ["references-view.findReferences"]
    }
  ],
  "vim.shell": "C:\\Users\\Admin\\AppData\\Local\\Microsoft\\WindowsApps\\pwsh.exe --nologo",
  "vim.timeout": 400,
  "vim.useSystemClipboard": true,
  "vim.vimrc.enable": true,
  "vim.vimrc.path": "D:\\Config\\nvim\\init.lua",
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "after": ["\"", "+", "y"],
      "before": ["<C-c>"]
    },
    {
      "after": ["\"", "+", "p"],
      "before": ["<C-v>"]
    },
    {
      "after": ["g", "j"],
      "before": ["j"]
    },
    {
      "after": ["g", "k"],
      "before": ["k"]
    }
  ],

